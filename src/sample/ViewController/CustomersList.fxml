<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewController.CustomersList">
   <children>
      <VBox layoutX="75.0" layoutY="100.0" prefHeight="200.0" prefWidth="448.0">
         <children>
            <TableView id="customer_list" prefHeight="200.0" prefWidth="283.0">
              <columns>
                <TableColumn id="customer_name" prefWidth="124.0" text="Customer Name" />
                <TableColumn id="appointment_title" prefWidth="126.0" text="Appointment Title" />
              </columns>
            </TableView>
            <HBox prefHeight="100.0" prefWidth="200.0" spacing="10.0">
                <children>
                    <Button id="edit_button" mnemonicParsing="false" text="Edit" />
                    <Button id="new_button" mnemonicParsing="false" text="New" />
                    <Button id="delete_button" mnemonicParsing="false" text="Delete" />
                </children>
               <VBox.margin>
                  <Insets top="10.0" />
               </VBox.margin>
               <padding>
                  <Insets top="10.0" />
               </padding>
            </HBox>
         </children>
      </VBox>
      <Label layoutX="75.0" layoutY="83.0" text="List of Customers" />

   </children>

</AnchorPane>
