<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewController.UsersController">
   <children>
      <VBox layoutX="250.0" layoutY="83.0" prefHeight="200.0" prefWidth="100.0">
         <children>
            <TextField id="user" fx:id="user" onAction="#onActionVerifyUser" promptText="Enter User" />
            <TextField id="password" fx:id="password" onAction="#onActionVerifyUser" promptText="Enter Password" />
            <Button fx:id="login" mnemonicParsing="false" onAction="#onActionVerifyUser" onKeyPressed="#onActionVerifyUser" text="Login" />
         </children>
      </VBox>
      <Label layoutX="181.0" layoutY="83.0" text="User ID:" />
      <Label layoutX="181.0" layoutY="109.0" text="Password:" />
      <Label fx:id="myTimeZone" layoutX="181.0" layoutY="166.0" text="No Timezone Found" />
      <Button fx:id="backToReality" layoutX="40.0" layoutY="14.0" mnemonicParsing="false" onAction="#backToMainController" text="Back" />
   </children>
</AnchorPane>
