<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewController.UsersController">
   <children>
      <Button fx:id="backToReality" mnemonicParsing="false" onAction="#backToMainController" text="Back" />
      <HBox layoutY="40.0" prefHeight="360.0" prefWidth="600.0">
         <children>
            <VBox prefHeight="360.0" prefWidth="147.0">
               <children>
                  <Label text="%user" />
                  <Label text="%login" />
               </children>
            </VBox>
            <VBox prefHeight="360.0" prefWidth="259.0">
               <children>
                  <TextField id="user" fx:id="user" onAction="#onActionVerifyUser" promptText="%EnterUser" />
                  <TextField id="password" fx:id="password" onAction="#onActionVerifyUser" promptText="%EnterPassword" />
                  <Button fx:id="login" mnemonicParsing="false" onAction="#onActionVerifyUser" onKeyPressed="#onActionVerifyUser" text="%submit" />
                  <Label fx:id="textTimeZone" text="%tz" />
                  <Label fx:id="myTimeZone" text="%timeZone" />
               </children>
            </VBox>
         </children>
      </HBox>
   </children>
</AnchorPane>
