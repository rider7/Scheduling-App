<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.ViewController.ContactsController">
   <children>
      <VBox layoutX="5.0" layoutY="6.0" prefHeight="387.0" prefWidth="590.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="205.0">
               <children>
                  <Label text="Customer Name:" />
                  <TextField id="customer_name" />
               </children>
            </HBox>

              <HBox prefHeight="100.0" prefWidth="205.0">
                  <children>
                      <Label text="Address:" />
                      <TextField id="address" />
                  </children>
              </HBox>
              <HBox prefHeight="100.0" prefWidth="205.0">
                  <children>
                      <Label text="Postal Code:" />
                      <TextField id="postal_code" />
                  </children>
              </HBox>
              <HBox prefHeight="100.0" prefWidth="205.0">
                  <children>
                      <Label text="Phone Number:" />
                      <TextField id="phone_number" />
                  </children>
              </HBox>
             <HBox prefHeight="100.0" prefWidth="200.0">
                 <children>
                     <ComboBox id="country_combo" prefHeight="21.0" prefWidth="261.0" promptText="Country" />
                     <ComboBox id="division_combo" prefHeight="25.0" prefWidth="389.0" promptText="Division" />
                 </children>
             </HBox>
             <Button id="edit_button" mnemonicParsing="false" text="Edit" />
          </children>
      </VBox>
   </children>
</AnchorPane>
